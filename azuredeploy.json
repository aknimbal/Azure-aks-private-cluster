{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vNETName": {
      "type": "String",
      "defaultValue": "wbpocAPPVnet",
      "metadata": {
        "description": "Name of the virtual network to be created"
      }
    },
    "addressRange": {
      "type": "String",
      "defaultValue":"10.0.0.0/8",
      "metadata": {
        "description": "Administrator password for the local admin account"
      }
    },
    "apimRange": {
      "type": "String",
      "defaultValue": "10.0.1.0/24"
    },
    "aksRange": {
      "type": "String",
      "defaultValue": "10.1.0.0/16"
    },
    "privateEPRange": {
      "type": "String",
      "defaultValue":"10.0.2.0/24"
    },
    "sharedRange": {
      "type": "String",
      "defaultValue": "10.0.3.0/24"
    },
    "bastionRange": {
      "type": "String",
      "defaultValue": "10.0.4.0/24"
    },
    "appgwRange": {
      "type": "string",
      "defaultValue": "10.0.5.0/24"
    },
    "appInsightsName": {
      "defaultValue": "wbpocapimai",
      "type": "string"
    },
    "apimname": {
      "type": "String",
      "defaultValue": "wbpocAPPapim"
    },
    "apimsku": {
      "type": "string",
      "defaultValue": "Developer"
    },
    "apimcapacity": {
      "type": "int",
      "defaultValue": 1
    },
    "apimEmail": {
      "type": "string",
      "defaultValue": "bwatts@microsoft.com"
    },
    "applicationGatewayName": {
      "type": "string",
      "defaultValue": "wbpocAPPappgw"
    },
    "appgwtier": {
      "type": "string",
      "defaultValue": "WAF_v2"
    },
    "appgwskuSize": {
        "type": "string",
        "defaultValue": "WAF_v2"
    },
    "appgwcapacity": {
        "type": "int",
        "defaultValue": 2
    },
    "appgwzones": {
        "type": "array",
        "defaultValue": [
                "1",
                "2",
                "3"
        ]
    },
    "appgwPIPName": {
        "type": "string",
        "defaultValue": "wbpocAPPappgw-pip"
    },
    "appgwsku": {
        "type": "string",
        "defaultValue": "Standard"
    },
    "appgwpublicIpZones": {
        "type": "array",
        "defaultValue": []
    },
    "appgwprivateIpAddress": {
        "type": "string",
        "defaultValue": "10.0.5.4"
    },
    "appgwautoScaleMaxCapacity": {
        "type": "int",
        "defaultValue": 4
    },
    "bastionHostName": {
      "type": "string",
      "defaultValue": "wbpocdemobastion"
    },
    "jumpName": {
      "type": "string",
      "defaultValue": "wbpocdemojump"
    },
    "jumpSize": {
      "type": "string",
      "defaultValue": "Standard_DS1_v2"
    },
    "adminUsername": {
      "type": "string",
      "defaultValue": "jumpAdmin"
    },
    "adminPassword": {
      "type": "secureString",
      "defaultValue": "ABCabc1234"
    },
    "jumpSKU": {
      "type": "string",
      "defaultValue": "2019-Datacenter"
    },
    "identityName": {
      "type": "string",
      "defaultValue": "wbpocid"
    },
    "vaultName": {
      "type": "string",
      "defaultValue": "wbpodkv"
    },
    "secretName": {
      "type": "string",
      "defaultValue": "appgwCert"
    },
    "certificate": {
